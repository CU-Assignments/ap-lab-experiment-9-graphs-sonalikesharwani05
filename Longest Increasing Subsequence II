class Solution {
    public int lengthOfLIS(int[] nums, int k) {
        int[] temp = new int[nums.length];
        int ans = 1;
        Arrays.fill(temp, 1);
        for(int i = 1; i < nums.length; i++) {
            for(int j = 0; j < i; j++) {
                if(nums[i] > nums[j] && nums[i] - nums[j] <= k) {
                    temp[i] = Math.max(temp[i], temp[j] + 1);
                    ans = Math.max(temp[i], ans);
                }
            }
        }
        return ans;
    }
}
